{
    "name": "arkhas/livewire-datatable",
    "description": "A powerful datatable component for Laravel Livewire with filters, actions, and export capabilities",
    "keywords": [
        "laravel",
        "livewire",
        "datatable",
        "table",
        "flux"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Ferre",
            "email": "mathieu@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0|^4.0",
        "livewire/flux": "^1.0|^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Arkhas\\LivewireDatatable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arkhas\\LivewireDatatable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arkhas\\LivewireDatatable\\LivewireDatatableServiceProvider"
            ],
            "aliases": {
                "LivewireDatatable": "Arkhas\\LivewireDatatable\\Facades\\LivewireDatatable"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
